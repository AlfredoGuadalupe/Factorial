#include <stdio.h>

int main(){
        int b,i;
        int f=1;
        printf("Ingrese un numero\n");
        scanf("%d",&b);                           
        if(b==0)
                f=1;
        else
                for(i=1;i<=b;i++)
                        f*=i;
        printf("El factorial es %d",f);
        return 0;
}
